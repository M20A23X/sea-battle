{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write src/",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "pre-commit": "lint-staged",
    "pre-push": "yarn lint && yarn test:quick",
    "start": "nest start",
    "start:dev": "yarn start --watch --debug",
    "start:prod": "yarn build && node dist/index",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:coverage": "yarn test --coverage",
    "test:quick": "yarn test --forceExit --no-cache --bail=1",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/platform-socket.io": "^10.2.1",
    "@nestjs/swagger": "^7.1.7",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mysql2": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0"
  }
}
